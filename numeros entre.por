programa {
  funcao inicio() {
    inteiro i
    para(i = 0; i < 20; i++){
      escreva("*")
    }
    escreva("\n")
    escreva("numeros entre 1 e 5", "\n")
    para(i = 0; i < 20; i++){
      escreva("*")
    }
    escreva("\n") 
     para(i = 1; i <= 5; i++){
      escreva(i,"\n")
     }
     para(i = 0; i < 20; i++){
      escreva("*")
    }
  }
}
